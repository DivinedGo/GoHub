{
  "version": 2,
  "functions": {
    "backend/server.js": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.js"
    },
    {
      "src": "/puzzles$",
      "dest": "/frontend/puzzles/puzzles.html"
    },
    {
      "src": "/puzzle-collections/([^/]+)$",
      "dest": "/frontend/puzzles/puzzle-collection.html"
    },
    {
      "src": "/puzzles/([^/]+)$",
      "dest": "/frontend/puzzles/puzzle.html"
    },
    {
      "src": "/puzzlesadmin27988794$",
      "dest": "/frontend/puzzles/puzzlesadmin27988794.html"
    },
    {
      "src": "/puzzlesadmincreate27988794$",
      "dest": "/frontend/puzzles/puzzlesadmincreate27988794.html"
    },
    {
      "src": "/styles.css",
      "dest": "/frontend/styles.css"
    },
    {
      "src": "/(.*\\.(css|js|html|png|jpg|gif|ico))$",
      "dest": "/frontend/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/puzzles/puzzles.html"
    }
  ],
  "env": {
    "MONGODB_URI": "@mongodb_uri",
    "ADMIN_KEY": "@admin_key",
    "NODE_ENV": "production"
  }
}
